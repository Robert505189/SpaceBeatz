<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="anchorPane" fx:id="anchorPane" minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="spacebeatz.MainMenu">
   <children>
      <ImageView fx:id="spaceBeatzLogo" fitHeight="150.0" fitWidth="345.0" layoutX="228.0" layoutY="23.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../res/spacebeatzlogo.jpg" />
         </image>
      </ImageView>
      <Label fx:id="chooseSongLabel" layoutX="131.0" layoutY="215.0" text="Choose your music:">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label fx:id="chosenSong" layoutX="294.0" layoutY="219.0" onMousePressed="#browseButtonListener" prefHeight="20.0" prefWidth="314.0" text="No song chosen. . .">
         <font>
            <Font name="System Italic" size="14.0" />
         </font>
      </Label>
      <Button fx:id="browseButton" layoutX="608.0" layoutY="216.0" mnemonicParsing="false" onAction="#browseButtonListener" text="Browse" />
      <Button fx:id="playButton" layoutX="368.0" layoutY="368.0" mnemonicParsing="false" onAction="#playButtonListener" text="PLAY">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
